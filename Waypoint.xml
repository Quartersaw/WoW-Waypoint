	<Frame name = "WaypointFrame" toplevel = "true" enableMouse = "true" movable = "true" parent = "UIParent">
		<Scripts>
			<OnLoad>
				self:SetClampedToScreen(1);
				WaypointOnLoad();
			</OnLoad>
			<OnUpdate function = "WaypointOnUpdate"/>
		</Scripts>
	</Frame>
</Ui> 